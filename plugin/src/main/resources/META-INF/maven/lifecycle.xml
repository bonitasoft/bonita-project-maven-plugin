<lifecycles>
  <lifecycle>
    <id>analyze-dependencies</id>
    <phases>
      <phase>
        <id>process-resources</id>
        <executions>
          <execution>
            <goals>
              <goal>
                org.bonitasoft.maven:bonita-project-maven-plugin:analyze
              </goal>
            </goals>
            <configuration>
			  <outputFile>${bonita.analyze.outputFile}</outputFile>
              <validateDeps>false</validateDeps>
            </configuration>
          </execution>
        </executions>
      </phase>
    </phases>
  </lifecycle>
</lifecycles>
