        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.newAddress ){",
        "\treturn {",
        "\t\t//map newAddress variable to expected task contract input",
        "\t\tnewAddressInput: {",
        "\t\t\taddress: $data.newAddress.address !== undefined ? $data.newAddress.address : null,",
        "\t\t\taddress2: $data.newAddress.address2 !== undefined ? $data.newAddress.address2 : null,",
        "\t\t\tpostalCode: $data.newAddress.postalCode !== undefined ? $data.newAddress.postalCode : null,",
        "\t\t\tcity: $data.newAddress.city !== undefined ? $data.newAddress.city : null,",
        "\t\t\tcountry: $data.newAddress.country !== undefined ? $data.newAddress.country : null",
        "\t\t}",
        "\t}",
        "}"
      ],
